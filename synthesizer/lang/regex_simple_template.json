[
	{ "syntax" : [ 
		{ "program": [
				[ "$*(", "star_clause",  "$)", "concat_clause", "$*(", "star_clause", "$)", "concat_clause", "$*(", "star_clause", "$)", "concat_clause", "$*(", "star_clause", "$)" ],
				[ "1", "1", "0", "$*(", "star_clause", "$)", "no_template" ]
		] },
		{ "no_templates" : [
				[ "concat" ],
				[ "star" ],
				[ "char" ]
		] },
		{ "concat" : [
				[ "concat_clause","$ ", "no_templates" ]
		] },
		{ "concat_clause" : [
			[ "char" ],
			[ "star" ]
		] },
		{ "star" : [
			[ "$*( ", "star_clause", "$ )" ]
		] },
		{ "star_clause" : [
			[ "concat" ],
			[ "char" ]
		] },
		{ "char" : [
			["0"],
			["1"],
			["?"]
		] }
	] },

	{ "execution" : [
		{ "program" : "*( ? )" },
		{ "no_templates" : "*( ? )" },
		{ "concat" : "*( ? )" },
		{ "concat_clause" : "*( ? )" },
		{ "star" : "*( ? )" },
		{ "star_clause" : "*( ? )" },
		{ "char" : "?" }
	] }
]


