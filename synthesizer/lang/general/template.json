[
	{ "syntax" : [ 
		{ "program" : [
				[ "bool" ]
		] },
		{ "bool" : [
				[ "$(",	"bool",	"$||", "bool",	"$)" ],
				[ "$(",	"bool",	"$&&", "bool",	"$)" ],
				[ "$!(", "bool", "$)" ],
				[ "0" ],
				[ "1" ],
				[ "any"	]
		] }
	] },

	{ "positive_placeholder" : [
		{ "program" : "any" },
		{ "bool" : "any" }
	] },

	{ "negative_placeholder" : [
		{ "program" : "0" },
		{ "bool" : "0" }
	] },

	{ "conditional_redundant" : [
		{ "(@1(bool) && @2(bool))" : {
			"--" : ["(@1 && !(@2))", "(!(@1) && @2)"],
			"==" : "EAR"
		} }
	] },
	
	{ "unconditional_redundant" : [
		{ "(@1(bool) && 0)" : "0" },
		{ "(@1(bool) && 1)" : "@1" }
	] }
]

