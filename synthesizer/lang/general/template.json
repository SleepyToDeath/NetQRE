[
	{ "syntax" : [ 
		{ "program" : [
				[ "bool" ]
		] },
		{ "bool" : [
				[ "$(",	"bool",	"$||", "bool",	"$)" ],
				[ "$(",	"bool",	"$&&", "bool",	"$)" ],
				[ "$!(", "bool", "$)" ],
				[ "0" ],
				[ "1" ],
				[ "any"	]
		] }
	] },

	{ "positive_abstract" : [
		{ "program" : "any" },
		{ "bool" : "any" }
	] },

	{ "negative_abstract" : [
		{ "program" : "0" },
		{ "bool" : "0" }
	] },

	{ "conditional_redundancy" : [
		{ "bool(bool@1 && bool@2)" : {
			"--" : ["bool(@1 && bool!(@2))", "bool(bool!(@1) && @2)"],
			"==" : "EAR"
		} }
	] },
	
	{ "unconditional_redundancy" : [
		{ "bool(bool@1 && 0)" : "0" },
		{ "bool(bool@1 && 1)" : "@1" }
	] }
]

